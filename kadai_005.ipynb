      "source": [
        "# 台形の面積を計算するプログラム\n",
        "上辺 = 10  #上辺の長さ（㎝）\n",
        "下辺 = 20  #下辺の長さ（㎝）\n",
        "高さ = 5   #高さ（㎝）\n",
        "\n",
        "#台形の面積を計算\n",
        "面積 = (上辺 + 下辺) * 高さ / 2\n",
        "\n",
        "#結果を出力\n",
        "print(\"台形の面積は\", 面積,\"㎠です\")\n"
      ]
    },
